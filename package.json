{
    "name": "nodejs-express-jest-tdd",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "start": "node ./bin/www",
        "serve": "nodemon app.js",
        "test": "jest --watchAll",
        "lint": "eslint . --fix"
    },
    "dependencies": {
        "cookie-parser": "~1.4.4",
        "debug": "~2.6.9",
        "express": "~4.16.1",
        "http-errors": "^2.0.0",
        "morgan": "~1.9.1"
    },
    "devDependencies": {
        "eslint": "^8.11.0",
        "jest": "^27.5.1",
        "nodemon": "^2.0.15",
        "supertest": "^6.2.2"
    },
    "eslintConfig": {
        "parserOptions": {
            "ecmaVersion": 8
        },
        "extends": "eslint:recommended",
        "env": {
            "node": true,
            "es6": true,
            "jest": true
        },
        "rules": {
            "semi": "warn",
            "quotes": [
                "warn",
                "single"
            ]
        }
    }
}
